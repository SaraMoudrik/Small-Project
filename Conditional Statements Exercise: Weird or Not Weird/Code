import random

if __name__ == '__main__':
    n = int(input("Entrez un num ").strip())
    if n in range(1,101):
        if n % 2 != 0:
            print("Weird")
        elif n in range(2,6):
            print("Not Weird")
        elif n in range(6,21):
            print("Weird")
        else:
            print("Not Weird")
    else:
        print("Le nombre doit Ãªtre compris entre 1 et 100 inclus.")

    print(n)
